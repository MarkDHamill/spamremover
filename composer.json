{
    "name": "phpbbservices/spamremover",
    "type": "phpbb-extension",
    "description": "Removes spam with a valid Akismet license key",
    "homepage": "https://www.phpbbservices.com/other/spamremover",
    "version": "1.0.0-dev",
    "time": "2021-01-23",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "MarkDHamill",
            "email": "mark@phpbbservices.com",
            "homepage": "https://www.phpbbservices.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Spam remover",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.0@dev"
        },
        "version-check": {
            "host": "www.phpbbservices.com",
            "directory": "/other/spamremover/updatecheck/",
            "filename": "3.3.json",
            "ssl": true
        }
    }
}
